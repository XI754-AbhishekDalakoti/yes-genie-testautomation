{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "consumer",
    "provider",
    "interactions",
    "metadata"
  ],
  "properties": {
    "consumer": {
      "$id": "#/properties/consumer",
      "type": "object",
      "title": "The Consumer Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/consumer/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Abhishek"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "provider": {
      "$id": "#/properties/provider",
      "type": "object",
      "title": "The Provider Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/provider/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Akshay"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "interactions": {
      "$id": "#/properties/interactions",
      "type": "array",
      "title": "The Interactions Schema",
      "items": {
        "$id": "#/properties/interactions/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "providerState",
          "contractId",
          "request",
          "response"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/interactions/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "My test"
            ],
            "pattern": "^(.*)$"
          },
          "providerState": {
            "$id": "#/properties/interactions/items/properties/providerState",
            "type": "string",
            "title": "The Providerstate Schema",
            "default": "",
            "examples": [
              "User "
            ],
            "pattern": "^(.*)$"
          },
          "contractId": {
            "$id": "#/properties/interactions/items/properties/contractId",
            "type": "integer",
            "title": "The Contractid Schema",
            "default": 0,
            "examples": [
              8001
            ]
          },
          "request": {
            "$id": "#/properties/interactions/items/properties/request",
            "type": "object",
            "title": "The Request Schema",
            "required": [
              "method",
              "path",
              "headers",
              "body"
            ],
            "properties": {
              "method": {
                "$id": "#/properties/interactions/items/properties/request/properties/method",
                "type": "string",
                "title": "The Method Schema",
                "default": "",
                "examples": [
                  "POST"
                ],
                "pattern": "^(.*)$"
              },
              "path": {
                "$id": "#/properties/interactions/items/properties/request/properties/path",
                "type": "string",
                "title": "The Path Schema",
                "default": "",
                "examples": [
                  "/users/search"
                ],
                "pattern": "^(.*)$"
              },
              "headers": {
                "$id": "#/properties/interactions/items/properties/request/properties/headers",
                "type": "object",
                "title": "The Headers Schema",
                "required": [
                  "Content-Type"
                ],
                "properties": {
                  "Content-Type": {
                    "$id": "#/properties/interactions/items/properties/request/properties/headers/properties/Content-Type",
                    "type": "string",
                    "title": "The Content-type Schema",
                    "default": "",
                    "examples": [
                      "application/json"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "body": {
                "$id": "#/properties/interactions/items/properties/request/properties/body",
                "type": "object",
                "title": "The Body Schema",
                "required": [
                  "searchQuery",
                  "threshold"
                ],
                "properties": {
                  "searchQuery": {
                    "$id": "#/properties/interactions/items/properties/request/properties/body/properties/searchQuery",
                    "type": "string",
                    "title": "The Searchquery Schema",
                    "default": "",
                    "examples": [
                      "Abhishek"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "threshold": {
                    "$id": "#/properties/interactions/items/properties/request/properties/body/properties/threshold",
                    "type": "integer",
                    "title": "The Threshold Schema",
                    "default": 0,
                    "examples": [
                      50
                    ]
                  }
                }
              }
            }
          },
          "response": {
            "$id": "#/properties/interactions/items/properties/response",
            "type": "object",
            "title": "The Response Schema",
            "required": [
              "status",
              "records",
              "count",
              "searchParams"
            ],
            "properties": {
              "status": {
                "$id": "#/properties/interactions/items/properties/response/properties/status",
                "type": "integer",
                "title": "The Status Schema",
                "default": 0,
                "examples": [
                  200
                ]
              },
              "records": {
                "$id": "#/properties/interactions/items/properties/response/properties/records",
                "type": "array",
                "title": "The Records Schema",
                "items": {
                  "$id": "#/properties/interactions/items/properties/response/properties/records/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "mdmId",
                    "custId",
                    "customerName",
                    "customerType",
                    "city",
                    "addressArea",
                    "homeBranch",
                    "assetRM",
                    "liabilityRM",
                    "groupId",
                    "businessSegment",
                    "partnerSegment",
                    "customerOpenDate",
                    "customerSearchEnabled",
                    "created",
                    "DOB"
                  ],
                  "properties": {
                    "mdmId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/mdmId",
                      "type": "integer",
                      "title": "The Mdmid Schema",
                      "default": 0,
                      "examples": [
                        838100
                      ]
                    },
                    "custId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/custId",
                      "type": "integer",
                      "title": "The Custid Schema",
                      "default": 0,
                      "examples": [
                        838236
                      ]
                    },
                    "customerName": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/customerName",
                      "type": "string",
                      "title": "The Customername Schema",
                      "default": "",
                      "examples": [
                        "chandan"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "customerType": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/customerType",
                      "type": "string",
                      "title": "The Customertype Schema",
                      "default": "",
                      "examples": [
                        "I"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "city": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/city",
                      "type": "string",
                      "title": "The City Schema",
                      "default": "",
                      "examples": [
                        "Ambala Kant"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "addressArea": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/addressArea",
                      "type": "string",
                      "title": "The Addressarea Schema",
                      "default": "",
                      "examples": [
                        "sector 23"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "homeBranch": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/homeBranch",
                      "type": "string",
                      "title": "The Homebranch Schema",
                      "default": "",
                      "examples": [
                        "09899"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "assetRM": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/assetRM",
                      "type": "string",
                      "title": "The Assetrm Schema",
                      "default": "",
                      "examples": [
                        "customer25"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "liabilityRM": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/liabilityRM",
                      "type": "string",
                      "title": "The Liabilityrm Schema",
                      "default": "",
                      "examples": [
                        "rahul customer35"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "groupId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/groupId",
                      "type": "string",
                      "title": "The Groupid Schema",
                      "default": "",
                      "examples": [
                        "274334"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "businessSegment": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/businessSegment",
                      "type": "string",
                      "title": "The Businesssegment Schema",
                      "default": "",
                      "examples": [
                        "brb"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "partnerSegment": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/partnerSegment",
                      "type": "string",
                      "title": "The Partnersegment Schema",
                      "default": "",
                      "examples": [
                        "brb"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "customerOpenDate": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/customerOpenDate",
                      "type": "string",
                      "title": "The Customeropendate Schema",
                      "default": "",
                      "examples": [
                        "03/05/2011"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "customerSearchEnabled": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/customerSearchEnabled",
                      "type": "boolean",
                      "title": "The Customersearchenabled Schema",
                      "default": false,
                      "examples": [
                        false
                      ]
                    },
                    "created": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/created",
                      "type": "string",
                      "title": "The Created Schema",
                      "default": "",
                      "examples": [
                        "03/05/2011"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "DOB": {
                      "$id": "#/properties/interactions/items/properties/response/properties/records/items/properties/DOB",
                      "type": "string",
                      "title": "The Dob Schema",
                      "default": "",
                      "examples": [
                        "08-Jan-2003"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              },
              "count": {
                "$id": "#/properties/interactions/items/properties/response/properties/count",
                "type": "integer",
                "title": "The Count Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "searchParams": {
                "$id": "#/properties/interactions/items/properties/response/properties/searchParams",
                "type": "array",
                "title": "The Searchparams Schema",
                "items": {
                  "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "mdmId",
                    "mobileNumber",
                    "PAN",
                    "passportNo",
                    "customerName",
                    "custId",
                    "cKyc",
                    "accountNo",
                    "aadharNo",
                    "branchCode",
                    "upiId",
                    "email",
                    "DOB",
                    "city"
                  ],
                  "properties": {
                    "mdmId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/mdmId",
                      "type": "integer",
                      "title": "The Mdmid Schema",
                      "default": 0,
                      "examples": [
                        809725
                      ]
                    },
                    "mobileNumber": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/mobileNumber",
                      "type": "integer",
                      "title": "The Mobilenumber Schema",
                      "default": 0,
                      "examples": [
                        8123456545
                      ]
                    },
                    "PAN": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/PAN",
                      "type": "string",
                      "title": "The Pan Schema",
                      "default": "",
                      "examples": [
                        "AXEPD7158N"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "passportNo": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/passportNo",
                      "type": "string",
                      "title": "The Passportno Schema",
                      "default": "",
                      "examples": [
                        "K4965650"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "customerName": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/customerName",
                      "type": "string",
                      "title": "The Customername Schema",
                      "default": "",
                      "examples": [
                        "Akshay"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "custId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/custId",
                      "type": "string",
                      "title": "The Custid Schema",
                      "default": "",
                      "examples": [
                        "838295"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "cKyc": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/cKyc",
                      "type": "string",
                      "title": "The Ckyc Schema",
                      "default": "",
                      "examples": [
                        "12345678907612"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "accountNo": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/accountNo",
                      "type": "string",
                      "title": "The Accountno Schema",
                      "default": "",
                      "examples": [
                        "123432345678912"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "aadharNo": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/aadharNo",
                      "type": "string",
                      "title": "The Aadharno Schema",
                      "default": "",
                      "examples": [
                        "123456789076"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "branchCode": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/branchCode",
                      "type": "string",
                      "title": "The Branchcode Schema",
                      "default": "",
                      "examples": [
                        "123"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "upiId": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/upiId",
                      "type": "string",
                      "title": "The Upiid Schema",
                      "default": "",
                      "examples": [
                        "abc@yes"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "email": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/email",
                      "type": "string",
                      "title": "The Email Schema",
                      "default": "",
                      "examples": [
                        "abc@yes.com"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "DOB": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/DOB",
                      "type": "string",
                      "title": "The Dob Schema",
                      "default": "",
                      "examples": [
                        "08-Jan-2003"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "city": {
                      "$id": "#/properties/interactions/items/properties/response/properties/searchParams/items/properties/city",
                      "type": "string",
                      "title": "The City Schema",
                      "default": "",
                      "examples": [
                        "Mumbai"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "metadata": {
      "$id": "#/properties/metadata",
      "type": "object",
      "title": "The Metadata Schema",
      "required": [
        "pactSpecification"
      ],
      "properties": {
        "pactSpecification": {
          "$id": "#/properties/metadata/properties/pactSpecification",
          "type": "object",
          "title": "The Pactspecification Schema",
          "required": [
            "version"
          ],
          "properties": {
            "version": {
              "$id": "#/properties/metadata/properties/pactSpecification/properties/version",
              "type": "string",
              "title": "The Version Schema",
              "default": "",
              "examples": [
                "2.0.0"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}